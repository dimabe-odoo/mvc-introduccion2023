@model List<mvc.Models.Entities.Job>
<table class="table table-hover">
    <tr>
        <th>Nombre</th>
        <th>Descripción</th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.JobName</td>
            <td>@item.JobDescription</td>
            <td>
                <a href="/jobs/delete/@item.JobId">Eliminar</a>
            </td>
        </tr>
    }
</table>